    <body>

        <div id="preloader">
            <div id="status">&nbsp;</div>
        </div>
       
        
        <!-- End page header -->

        <!-- property area -->
        <div class="content-area single-property" style="background-color: #FCFCFC;">&nbsp;
            <div class="container">   

                <div class="clearfix padding-top-40" >

                    <div class="col-md-8 single-property-content prp-style-2">
                        <div class="">
                            <div class="row">
                                <div class="light-slide-item">            
                                    <div class="clearfix">
                                        <div class="favorite-and-print">
                                            
                                            <a class="printer-icon " href="javascript:window.print()">
                                                <i class="fa fa-print"></i> 
                                            </a>
                                        </div> 

                                        <ul id="image-gallery" class="gallery list-unstyled cS-hidden">
                                            <li data-thumb="<%= propterites.previewPicture %>"> 
                                                <img src="<%= propterites.previewPicture %>" />
                                            </li>
                                            <% picListing.forEach(function(pic) { %>
                                            <li data-thumb="<%= pic %>"> 
                                                <img src="<%= pic %>" />
                                            </li>
                                            <% }); %>
                                            <!-- <li data-thumb="/img/property-1/property3.jpg"> 
                                                <img src="/img/property-1/property3.jpg" />
                                            </li>
                                            <li data-thumb="/img/property-1/property4.jpg"> 
                                                <img src="/img/property-1/property4.jpg" />
                                            </li>                                          -->
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="single-property-wrapper">

                                <div class="section">
                                    <h4 class="s-property-title">Description</h4>
                                    <div class="s-property-content">
                                        <p><%= propterites.detailsDescribe %></p>
                                    </div>
                                </div>
                                <!-- End description area  -->

                                <div class="section additional-details">

                                    <h4 class="s-property-title">Additional Details</h4>

                                    <ul class="additional-details-list clearfix">
                                        <li>
                                            <span class="col-xs-6 col-sm-4 col-md-4 add-d-title">Waterfront</span>
                                            <span class="col-xs-6 col-sm-8 col-md-8 add-d-entry"><%= propterites.waterfrontDescription %></span>
                                        </li>

                                        <li>
                                            <span class="col-xs-6 col-sm-4 col-md-4 add-d-title">Built In</span>
                                            <span class="col-xs-6 col-sm-8 col-md-8 add-d-entry"><%= propterites.builtYear %></span>
                                        </li>
                                        <li>
                                            <span class="col-xs-6 col-sm-4 col-md-4 add-d-title">Parking</span>
                                            <span class="col-xs-6 col-sm-8 col-md-8 add-d-entry"></span>
                                        </li>

                                        <li>
                                            <span class="col-xs-6 col-sm-4 col-md-4 add-d-title">Community Features</span>
                                            <span class="col-xs-6 col-sm-8 col-md-8 add-d-entry"><%= propterites.communityFeatures %></span>
                                        </li>

                                        <li>
                                            <span class="col-xs-6 col-sm-4 col-md-4 add-d-title">View</span>
                                            <span class="col-xs-6 col-sm-8 col-md-8 add-d-entry"><%= propterites.view %></span>
                                        </li>

                                        <li>
                                            <span class="col-xs-6 col-sm-4 col-md-4 add-d-title">chimeAmenities:</span>
                                            <span class="col-xs-6 col-sm-8 col-md-8 add-d-entry"><%= propterites.chimeAmenities %></span>
                                        </li> 
                                        <li>
                                            <span class="col-xs-6 col-sm-4 col-md-4 add-d-title">Elementary School:</span>
                                            <span class="col-xs-6 col-sm-8 col-md-8 add-d-entry"><%= propterites.elementarySchool %></span>
                                        </li>
                                        <li>
                                            <span class="col-xs-6 col-sm-4 col-md-4 add-d-title">Middle School:</span>
                                            <span class="col-xs-6 col-sm-8 col-md-8 add-d-entry"><%= propterites.middleSchool %></span>
                                        </li>
                                        <li>
                                            <span class="col-xs-6 col-sm-4 col-md-4 add-d-title">High School:</span>
                                            <span class="col-xs-6 col-sm-8 col-md-8 add-d-entry"><%= propterites.highSchool %></span>
                                        </li> 
                                        <li>
                                            <span class="col-xs-6 col-sm-4 col-md-4 add-d-title">Other School:</span>
                                            <span class="col-xs-6 col-sm-8 col-md-8 add-d-entry"><%= propterites.otherSchool %></span>
                                        </li> 
                                        <li>
                                            <span class="col-xs-6 col-sm-4 col-md-4 add-d-title">District School:</span>
                                            <span class="col-xs-6 col-sm-8 col-md-8 add-d-entry"><%= propterites.schoolDistrict %></span>
                                        </li>

                                    </ul>
                                </div>  
                                <!-- End additional-details area  

                                <div class="section property-features">      

                                    <h4 class="s-property-title">Features</h4>                            
                                    <ul>
                                        <li><a href="properties.html">Swimming Pool</a></li>   
                                        <li><a href="properties.html">3 Stories</a></li>
                                        <li><a href="properties.html">Central Cooling</a></li>
                                        <li><a href="properties.html">Jog Path 2</a></li>
                                        <li><a href="properties.html">2 Lawn</a></li>
                                        <li><a href="properties.html">Bike Path</a></li>
                                    </ul>

                                </div>
                                End features area  -->

                            </div>
                        </div>

                        </div>

                    <div class="col-md-4 p0">
                        <aside class="sidebar sidebar-property blog-asside-right property-style2">
                            <div class="dealer-widget">
                                <div class="dealer-content">
                                    <div class="inner-wrapper">
                                        <div class="single-property-header">                                          
                                            <h1 class="property-title"><%= propterites.propertyType %></h1>
                                            <span class="property-price">$<%= propterites.price %></span>
                                        </div>

                                        <div class="property-meta entry-meta clearfix ">   

                                            <div class="col-xs-4 col-sm-4 col-md-4 p-b-15">
                                                <span class="property-info-icon icon-tag">                                                                                      
                                                    <img src="/img/icon/sale-orange.png">
                                                </span>
                                                <span class="property-info-entry">
                                                    <span class="property-info-label">Status</span>
                                                    <span class="property-info-value"><%= propterites.purchaseType %></span>
                                                </span>
                                            </div>

                                            <div class="col-xs-4 col-sm-4 col-md-4 p-b-15">
                                                <span class="property-info icon-area">
                                                    <img src="/img/icon/room-orange.png">
                                                </span>
                                                <span class="property-info-entry">
                                                    <span class="property-info-label">Area</span>
                                                    <span class="property-info-value"><%= propterites.sqft %><b class="property-info-unit">Sq Ft</b></span>
                                                </span>
                                            </div>

                                            <div class="col-xs-4 col-sm-4 col-md-4 p-b-15">
                                                <span class="property-info-icon icon-bed">
                                                    <img src="/img/icon/bed-orange.png">
                                                </span>
                                                <span class="property-info-entry">
                                                    <span class="property-info-label">Bedrooms</span>
                                                    <span class="property-info-value"><%= propterites.bedrooms %></span>
                                                </span>
                                            </div>

                                            <div class="col-xs-4 col-sm-4 col-md-4 p-b-15">
                                                <span class="property-info-icon icon-bath">
                                                    <img src="/img/icon/cars-orange.png">
                                                </span>
                                                <span class="property-info-entry">
                                                    <span class="property-info-label">Bathrooms</span>
                                                    <span class="property-info-value"><%= propterites.bathrooms %></span>
                                                </span>
                                            </div>
                                            <!-- <div class="col-xs-4 col-sm-4 col-md-4 p-b-15">
                                                <span class="property-info-icon icon-bath">
                                                    <img src="/img/icon/os-orange.png">
                                                </span>
                                                
                                            </div> -->

                                            <div class="col-xs-4 col-sm-4 col-md-4 p-b-15">
                                                <span class="property-info-icon icon-garage">
                                                    <img src="/img/icon/room-orange.png">
                                                </span>
                                                <span class="property-info-entry">
                                                    <span class="property-info-label">Garages</span>
                                                    <span class="property-info-value"><%= propterites.garageSpaceDescription %></span>
                                                </span>
                                            </div>

                                            <!-- <div class="col-xs-4 col-sm-4 col-md-4 p-b-15">
                                                <span class="property-info-icon icon-garage">
                                                    <img src="/img/icon/shawer-orange.png">
                                                </span>
                                                <span class="property-info-entry">
                                                    <span class="property-info-label">Garages</span>
                                                    <span class="property-info-value"><%= propterites.garageSpaceDescription %></span>
                                                </span>
                                            </div> -->


                                        </div>
                                        <div class="dealer-section-space">
                                            <span>Dealer information</span>
                                        </div>
                                        <div class="clear">
                                            <!-- <div class="col-xs-4 col-sm-4 dealer-face">
                                                <a href="">
                                                    <img src="/img/client-face1.png" class="img-circle">
                                                </a>
                                            </div> -->
                                            <div class="col-xs-8 col-sm-8 ">
                                                <h3 class="dealer-name">
                                                    <a href=""><%= propterites.agentName %></a>
                                                    <span>Real Estate Agent</span>        
                                                </h3>
                                                <!-- <div class="dealer-social-media">
                                                    <a class="twitter" target="_blank" href="">
                                                        <i class="fa fa-twitter"></i>
                                                    </a>
                                                    <a class="facebook" target="_blank" href="">
                                                        <i class="fa fa-facebook"></i>
                                                    </a>
                                                    <a class="gplus" target="_blank" href="">
                                                        <i class="fa fa-google-plus"></i>
                                                    </a>
                                                    <a class="linkedin" target="_blank" href="">
                                                        <i class="fa fa-linkedin"></i>
                                                    </a> 
                                                    <a class="instagram" target="_blank" href="">
                                                        <i class="fa fa-instagram"></i>
                                                    </a>       
                                                </div> -->

                                            </div>
                                        </div>

                                        <div class="clear">
                                            <ul class="dealer-contacts">                                       
                                                <li><i class="pe-7s-map-marker strong"> </i> <%= propterites.address %></li>
                                                <!-- <li><i class="pe-7s-mail strong"> </i> <%= propterites.address %></li>
                                                <li><i class="pe-7s-call strong"> </i> +1 908 967 5906</li> -->
                                            </ul>
                                            <p></p>
                                        </div>

                                    </div>
                                </div>
                            </div>


                            <div class="panel panel-default sidebar-menu wow fadeInRight animated">
                                <div class="panel-heading">
                                    <!-- <h3 class="panel-title">Ads her  </h3> -->
                                </div>
                                <div class="panel-body recent-property-widget">
                                    <iframe
                                        width="450"
                                        height="800"
                                        frameborder="0" style="border:0"
                                        referrerpolicy="no-referrer-when-downgrade"
                                        src="https://www.google.com/maps/embed/v1/place
                                        ?key=AIzaSyCnt3S_eGw5IPXkqxw6aaPNNg4496IYivM
                                        &q=Eiffel+Tower,Paris+France"
                                        allowfullscreen>
                                        </iframe>
                                </div>
                            </div>

                            </aside>
                    </div>

                </div>

            </div>
        </div>

        
        <script src="/js/vendor/modernizr-2.6.2.min.js"></script>
        <script src="/js/jquery-1.10.2.min.js"></script>
        <script src="bootstrap/js/bootstrap.min.js"></script>
        <script src="/js/bootstrap-select.min.js"></script>
        <script src="/js/bootstrap-hover-dropdown.js"></script>
        <script src="/js/easypiechart.min.js"></script>
        <script src="/js/jquery.easypiechart.min.js"></script>
        <script src="/js/owl.carousel.min.js"></script>
        <script src="/js/wow.js"></script>
        <script src="/js/icheck.min.js"></script>
        <script src="/js/price-range.js"></script>
        <script type="text/javascript" src="/js/lightslider.min.js"></script>
        <script src="/js/main.js"></script>

        <script>
                            $(document).ready(function () {

                                $('#image-gallery').lightSlider({
                                    gallery: true,
                                    item: 1,
                                    thumbItem: 9,
                                    slideMargin: 0,
                                    speed: 500,
                                    auto: true,
                                    loop: true,
                                    onSliderLoad: function () {
                                        $('#image-gallery').removeClass('cS-hidden');
                                    }
                                });
                            });
        </script>

    </body>
</html>